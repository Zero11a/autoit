GUISetState(@SW_ENABLE, $h2)
While 1
	Sleep(29)
	If GUICtrlGetState($ho) <> $65 Then GUICtrlSetState($ho, $65)
	_vc()
WEnd

Func _vc()
	Local $hr = _1q($gu)
	If @error Then Return SetError(1)
	Select 
		Case $hr == "playing"
			_vz()
		Case $hr == "stopped"
			If IsArray($gx) Then
				$gw = _d5(_d6($gw, $h0[0]), 1)
				If $gr = 1 Then
				ElseIf $gr = 2 Then
					$gr = 0
					_1k($gu)
				Else
					If $gp Then
						If GUICtrlRead($ho) <> $h0[$gw] Then
							GUICtrlSetState($h0[$gw], $65)
						Else
							_w0()
						EndIf
					Else
						$gw += 1
						If $gw > $h0[0] Then $gw = 1
						GUICtrlSetState($h0[$gw], $65)
					EndIf
				EndIf
			Else
				_1j($gu)
				_vu(0)
			EndIf
	EndSelect
	Return 1
EndFunc

Func _vd()
	_vc()
	If @error Then Return SetError(1)
	If NOT IsArray($h0) Then Return SetError(2)
	Local $hs = GUIGetMsg()
	Switch $hs
		Case $h0[1] To $h0[$h0[0]]
			_w0()
		Case Else
			Return $hs
	EndSwitch
EndFunc

Func _ve()
	Opt("GUIOnEventMode", 0)
	GUISwitch($h2)
	Local $ht, $hu, $hv, $hs, $hw, $hx
	GUICtrlSetState($ha, $62)
	$go = -1
	$hw = GUICtrlCreateLabel("", 10, 115, 0, 25, $5w + $5x)
	$hx = GUICtrlCreateLabel("", 10, 115, 70, 25, $5w + $5x)
	GUICtrlSetBkColor(-1, $gf)
	Do
		$ht = _d5(_d6(Int((MouseGetPos(0) - 10) / 70 * 100), 100), 0)
		If $ht <> $go Then
			$go = $ht
			$hu = Int($ht / 100 * 70)
			ControlMove($h2, "", $hw, 10, 115, $hu, 25)
			ControlMove($h2, "", $hx, 10 + $hu, 115, 70 - $hu, 25)
			_vs()
			If $go > 49 Then
				Dim $hv[3] = [$go, "", $gd]
			Else
				Dim $hv[3] = ["", $go, $ge]
			EndIf
			GUICtrlSetData($hw, $hv[0])
			GUICtrlSetData($hx, $hv[1])
			GUICtrlSetBkColor($hw, $hv[2])
		EndIf
		Sleep(29)
		$hs = _vd()
		If @error OR (NOT $hs) Then $hs = GUIGetMsg()
	Until $hs = $60
	_w7("Volume", $go)
	GUICtrlDelete($hw)
	GUICtrlDelete($hx)
	GUICtrlSetState($ha, $61)
	Opt("GUIOnEventMode", 1)
	Return 1
EndFunc

Func _vf()
	Local $hy = GUICtrlRead($hh)
	Switch $hy
		Case "Bắt đầu"
			If NOT IsArray($gx) Then Return SetError(1)
			If GUICtrlRead($ho) = $h0[1] Then
				_w0()
			Else
				GUICtrlSetState($h0[1], $65)
			EndIf
		Case "Tạm dừng"
			_1m($gu)
			_vu(1)
		Case "Tiếp tục"
			If $gr = 1 Then
				$gr = 2
			Else
				_1n($gu)
			EndIf
			_vu(2)
	EndSwitch
	Return 1
EndFunc

Func _vg()
	Opt("GUIOnEventMode", 0)
	GUISwitch($h2)
	GUICtrlSetState($hc, $62)
	Local $hs, $hz = 0
	Local $i0[3] = ["  Bài hát", "  Ca sĩ", "  Xáo trộn"]
	Local $i1[3] = [$ge, $gf, $gd]
	For $i = 0 To 2
		$i0[$i] = GUICtrlCreateLabel($i0[$i], 160, 85 + $i * 30, 70, 25, $5w)
		GUICtrlSetBkColor(-1, $i1[$i])
	Next
	Do
		Sleep(29)
		$hs = _vd()
		If @error OR (NOT $hs) Then $hs = GUIGetMsg()
		If $hs = $5y Then
			_w5()
		ElseIf $hs = $60 Then
			$hz = 1
		ElseIf $hz Then
			If $hs = $5z Then ExitLoop
		EndIf
	Until $i0[0] <= $hs AND $hs <= $i0[2]
	For $i = 0 To 2
		If $hs = $i0[$i] Then
			If $i <> $gn OR $i = 2 Then
				$gn = $i
				_vt()
				_w7("SortType", $gn)
			EndIf
			ExitLoop
		EndIf
	Next
	For $i = 0 To 2
		GUICtrlDelete($i0[$i])
	Next
	GUICtrlSetState($hc, $61)
	Opt("GUIOnEventMode", 1)
	Return 1
EndFunc

Func _vh()
	$gq = NOT $gq
	_vw()
	_w7("ShowLyricWindow", Number($gq))
	Return 1
EndFunc

Func _vi()
	Local $i2 = _vq("Sửa thư mục bài hát", "Sửa \MP3", "MP3Dir", $gl)
	If NOT _w2($i2, $gl) Then Return SetError(1)
	$gl = $i2
	_vo()
	Return 1
EndFunc

Func _vj()
	Local $i3 = _vq("Sửa thư mục lời bài hát", "Sửa \LRC", "LRCDir", $gm)
	If NOT _w2($i3, $gm) Then Return SetError(1)
	$gm = $i3
	_vy()
	Return 1
EndFunc

Func _vk()
	$gp = NOT $gp
	_vv()
	_w7("Repeat", Number($gp))
	Return 1
EndFunc

Func _vl()
	Local $i4 = _1q($gu)
	If @error Then Return SetError(1)
	Local $i5 = _1r($gu, 2)
	Local $i6 = 5000
	If @GUI_CtrlId = $h3 Then
		$i6 *= -1
		$gz = 1
	EndIf
	_w1($i5 + $i6)
	If $i4 == "playing" Then
		_1k($gu)
	Else
		$gr = 1
	EndIf
	Return 1
EndFunc

Func _vm()
	Local $i4 = _1q($gu)
	If @error Then Return SetError(1)
	Opt("GUIOnEventMode", 0)
	Local $i7, $i8, $i9 = 295
	Do
		Sleep(29)
		$i8 = _d5(_d6(MouseGetPos(0) - 160, $i9), 0)
		If $i8 <> $i7 Then
			If $i8 < $i7 Then $gz = 1
			$i7 = $i8
			$i8 /= $i9
			$i8 *= $gv
			_w1($i8)
		EndIf
	Until GUIGetMsg() = $60
	If $i4 == "playing" Then
		_1k($gu)
	Else
		$gr = 1
	EndIf
	Opt("GUIOnEventMode", 1)
	Return 1
EndFunc

Func _vn()
	_1q($gu)
	If @error Then Return SetError(1)
	Opt("GUIOnEventMode", 0)
	Local $ia = _v9($gx[$gw][2], $gx[$gw][3])
	Local $fq, $hs, $ib, $ic, $au, $c2, $id, $ie, $if, $ig, $ih = 0, $ii = 0, $ij = 0
	Local $ik = GUICreate("Tìm lời bài hát", 370, 285, -1, -1, -1, -1, $h2)
	GUISetFont($gh)
	Local $il = GUICtrlCreateLabel("Danh sách tìm thấy", 5, 5, 150, 25, $5w)
	GUICtrlSetFont(-1, $gi)
	Local $im = GUICtrlCreateListView("STT|Tên bài hát + Ca sĩ|Lượt nghe|Người tải lên", 10, 30, 355, 220, 0)
	Local $in = GUICtrlCreateLabel("Chọn", 5, 255, 70, 25, $5w + $5x)
	GUICtrlSetBkColor(-1, $ge)
	Local $io = GUICtrlCreateEdit("http://www.nhaccuatui.com/bai-hat/...", 10, 30, 355, 220)
	GUICtrlSetState(-1, $62)
	Local $ip = GUICtrlCreateLabel("Nhập URL", 80, 255, 70, 25, $5w + $5x)
	GUICtrlSetBkColor(-1, $ge)
	GUICtrlSetState(-1, $64)
	Local $iq = GUICtrlCreateLabel("Thoát", 295, 255, 70, 25, $5w + $5x)
	GUICtrlSetBkColor(-1, $ge)
	If IsArray($ia) Then
		Dim $ig[$ia[0][0]]
		For $i = 1 To $ia[0][0]
			$ig[$i - 1] = GUICtrlCreateListViewItem($i & "|" & $ia[$i][0] & "|" & $ia[$i][2] & "|" & $ia[$i][3], $im)
		Next
		GUICtrlSetState($ig[0], $65)
	EndIf
	GUISetState(@SW_SHOW)
	Do
		Sleep(29)
		$hs = _vd()
		If @error OR (NOT $hs) Then $hs = GUIGetMsg()
		If WinGetState($ik) <> 15 Then ExitLoop
		Switch $hs
			Case $5y, $iq
				ExitLoop
			Case $in
				If NOT IsArray($ia) Then ContinueLoop
				$ic = GUICtrlRead($im)
				If @error OR $ih Then
					GUICtrlSetState($im, $61)
					GUICtrlSetState($io, $62)
					GUICtrlSetData($ip, "Nhập URL")
					GUICtrlSetData($il, "Danh sách tìm thấy")
					GUICtrlSetState($im, $65)
					$ih = 0
					ContinueLoop
				Else
					$au = StringSplit(GUICtrlRead($ic, 2), "|", 2)
					If NOT @error Then $if = $ia[$au[0]][1]
					ExitLoop
				EndIf
			Case $ip
				If $ih Then
					$if = StringStripWS(GUICtrlRead($io), 7)
					If StringLeft($if, 7) = "http://" Then $if = StringTrimLeft($if, 26)
					ExitLoop
				Else
					GUICtrlSetState($im, $62)
					GUICtrlSetState($io, $61)
					GUICtrlSetData($ip, "Tải LRC")
					GUICtrlSetData($il, "Tự nhập URL")
					$ih = 1
				EndIf
		EndSwitch
	Until 0
	If $if Then
		$c2 = _jg(StringTrimRight($gx[$gw][0], 4) & ".lrc", $gm, $gx[$gw][2], $gx[$gw][3])
		If @error Then
			$ii = 1
		Else
			GUICtrlSetState($im, $62)
			GUICtrlSetState($io, $62)
			GUICtrlSetState($ip, $62)
			GUICtrlSetData($in, "Đồng ý")
			GUICtrlSetData($il, "Xác nhận tải")
			GUICtrlCreateLabel("Tệp lời bài hát đã tồn tại bạn vẫn muốn tải chứ?" & @CRLF & '- Ấn "Đồng ý" để tải' & @CRLF & '- Ấn "Thoát" để hủy', 10, 30, 355, 220)
			Do
				Sleep(29)
				$hs = _vd()
				If @error OR (NOT $hs) Then $hs = GUIGetMsg()
				If WinGetState($ik) <> 15 Then ExitLoop
				Switch $hs
					Case $in
						$ii = 1
						ContinueCase
					Case $in, $ip, $5y, $iq
						ExitLoop
				EndSwitch
			Until 0
		EndIf
		If $ii Then
			$ie = _va($if)
			If @error Then
				$ij = 3
			Else
				$c2 = $gm[0] & "\" & StringTrimRight($gx[$gw][0], 4) & ".lrc"
				$id = FileOpen($c2, 2)
				FileWrite($id, $ie)
				FileClose($id)
				_vy()
			EndIf
		EndIf
	EndIf
	GUIDelete($ik)
	Opt("GUIOnEventMode", 1)
	If $ij Then Return SetError($ij)
	Return 1
EndFunc

Func _vo()
	GUICtrlSetData($hn, "Đang tải danh sách bài hát")
	$gx = _j4($gl)
	_vt()
	Return 1
EndFunc

Func _vp()
	Opt("GUIOnEventMode", 0)
	Local $ir = GUICreate("Thông tin + Trợ giúp", 370, 285, -1, -1, -1, -1, $h2)
	GUISetFont($gh)
	Local $hs, $is = 0
	Local $it[2] = ["Phần mềm: Nghe nhạc cùng lời bài hát" & @CRLF & "Phiên bản: " & FileGetVersion(@ScriptFullPath) & @CRLF & "Người phát triển: Hoàng hoangdangkyok@fb" & @CRLF & "Những cập nhật ở phiên bản mới:" & @CRLF & "- Trải nghiệm: Tốt hơn (sử dụng + nghe nhạc + xem lời)" & @CRLF & "- Phím tắt: Thêm F1, F5, Left, Right,..." & @CRLF & "- Giao diện: Một vài thay đổi, ít + nhỏ" & @CRLF & "- Chức năng: Tải lời bài hát, tiến trình ở thanh công cụ" & @CRLF & "- Khác: Một vài thay đổi nhỏ khác *_*" & @CRLF & "Những hạn chế + lỗi + chức năng cần bổ sung:" & @CRLF & "- Phần này thì nhờ đóng góp của người dùng :)", '- Để bắt đầu, chọn "Sửa \MP3" để đổi thư mục chứa MP3' & @CRLF & '- Tiếp theo, chọn "Sửa \LRC" để đổi thư mục chứa LRC' & @CRLF & "- Chỉnh âm lượng bằng cách kéo thả thanh âm lượng" & @CRLF & "- Có ba loại sắp xếp: Theo bài hát, ca sĩ và xáo trộn" & @CRLF & "- Chức năng tải lời bài hát: Tự động hoặc Thủ công" & @CRLF & "  (Chức năng thủ công đã được hoàn tất phát triển" & @CRLF & "   nhưng mình đã vô hiệu vì một số lí do nào đó)" & @CRLF & "- Các phím tắt đang chờ bạn tìm hiểu :)" & @CRLF & "- Các chức năng còn lại cũng chờ bạn tìm hiểu :)"]
	Local $il = GUICtrlCreateLabel("Thông tin", 5, 5, 70, 25, $5w + $5x)
	GUICtrlSetFont(-1, $gi)
	Local $iu = GUICtrlCreateLabel($it[$is], 10, 30, 355, 225)
	Local $iv = GUICtrlCreateLabel("Trợ giúp", 5, 255, 70, 25, $5w + $5x)
	GUICtrlSetBkColor(-1, $ge)
	Local $iq = GUICtrlCreateLabel("Thoát", 295, 255, 70, 25, $5w + $5x)
	GUICtrlSetBkColor(-1, $ge)
	GUISetState(@SW_SHOW)
	Do
		Sleep(29)
		$hs = _vd()
		If @error OR (NOT $hs) Then $hs = GUIGetMsg()
		If WinGetState($ir) <> 15 Then ExitLoop
		Switch $hs
			Case $5y, $iq
				ExitLoop
			Case $iv
				$is = NOT $is
				If $is Then
					GUICtrlSetData($iv, "Thông tin")
					GUICtrlSetData($il, "Trợ giúp")
					GUICtrlSetData($iu, $it[$is])
				Else
					GUICtrlSetData($iv, "Trợ giúp")
					GUICtrlSetData($il, "Thông tin")
					GUICtrlSetData($iu, $it[$is])
				EndIf
		EndSwitch
	Until 0
	GUIDelete($ir)
	Opt("GUIOnEventMode", 1)
	Return 1
EndFunc

Func _vq($bv, $iw, $ix, $iy)
	Opt("GUIOnEventMode", 0)
	Local $iz, $hs, $j0, $ic, $au, $j1, $hq, $j2 = $iy
	Local $j3 = GUICreate($bv, 270, 285, -1, -1, -1, -1, $h2)
	GUISetFont($gh)
	GUICtrlCreateLabel($iw, 5, 5, 100, 25, $5w)
	GUICtrlSetFont(-1, $gi)
	Local $j4 = GUICtrlCreateTreeView(10, 30, 255, 220, 0)
	Local $j5 = GUICtrlCreateLabel("Thêm", 5, 255, 70, 25, $5w + $5x)
	GUICtrlSetBkColor(-1, $ge)
	Local $j6 = GUICtrlCreateLabel("Xóa", 80, 255, 70, 25, $5w + $5x)
	GUICtrlSetBkColor(-1, $ge)
	Local $iq = GUICtrlCreateLabel("Thoát", 195, 255, 70, 25, $5w + $5x)
	GUICtrlSetBkColor(-1, $ge)
	If UBound($iy) = 1 Then GUICtrlSetState($j6, $64)
	For $i = 0 To UBound($iy) - 1
		$j2[$i] = GUICtrlCreateTreeViewItem($iy[$i], $j4)
	Next
	GUICtrlSetState($j2[0], $65)
	GUISetState(@SW_SHOW)
	Dim $hq[2][2] = [["{Enter}", $j5], ["{Delete}", $j6]]
	GUISetAccelerators($hq)
	Do
		Sleep(29)
		$hs = _vd()
		If @error OR (NOT $hs) Then $hs = GUIGetMsg()
		If WinGetState($j3) <> 15 Then ExitLoop
		Switch $hs
			Case $5y, $iq
				ExitLoop
			Case $j5
				$j0 = FileSelectFolder($bv, "")
				If @error Then ContinueLoop
				$j1 = 1
				For $i = 0 To UBound($j2) - 1
					If $j0 = GUICtrlRead($j2[$i], 1) Then
						$j1 = 0
						ExitLoop
					EndIf
				Next
				If $j1 Then
					_0($j2, GUICtrlCreateTreeViewItem($j0, $j4))
					GUICtrlSetState($j6, $63)
				EndIf
			Case $j6
				If UBound($j2) = 1 Then ContinueLoop
				$ic = GUICtrlRead($j4)
				If @error Then ContinueLoop
				$au = _by($j4, $ic)
				_6($j2, $au)
				GUICtrlDelete($ic)
				If UBound($j2) = 1 Then GUICtrlSetState($j6, $64)
		EndSwitch
	Until 0
	For $i = 0 To UBound($j2) - 1
		$iz &= GUICtrlRead($j2[$i], 1) & "|"
	Next
	$iz = StringTrimRight($iz, 1)
	_w7($ix, $iz)
	GUIDelete($j3)
	Opt("GUIOnEventMode", 1)
	Return StringSplit($iz, "|", 2)
EndFunc

Func _vr()
	Local $hs, $j7, $i7, $i8
	$j7 = MouseGetPos(1)
	Opt("GUIOnEventMode", 0)
	Opt("MouseCoordMode", 1)
	Do
		$i8 = MouseGetPos(1)
		If $i8 <> $i7 Then
			$i7 = $i8
			WinMove($hp, "", 0, $i8 - $j7)
		EndIf
		Sleep(29)
		$hs = _vd()
		If @error OR (NOT $hs) Then $hs = GUIGetMsg()
	Until $hs = $60
	_w7("LyricWindowPos", $i8 - $j7)
	Opt("MouseCoordMode", 2)
	Opt("GUIOnEventMode", 1)
EndFunc

Func _vs()
	Local $i1 = $ge
	If $go > 49 Then $i1 = $gd
	GUICtrlSetBkColor($ha, $i1)
	GUICtrlSetTip($ha, "Âm lượng: " & $go)
	SoundSetWaveVolume($go)
EndFunc

Func _vt()
	Local $i1[3] = [$ge, $gf, $gd]
	GUICtrlSetBkColor($hc, $i1[$gn])
	If IsArray($gx) Then _j5($gx, $gn)
	_w8()
EndFunc

Func _vu($j8)
