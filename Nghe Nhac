Func _2j($4r, $4s, $4t = 0, $4u = 0, $4v = 0, $4w = "wparam", $4x = "lparam", $4y = "lresult")
	Local $4q = DllCall("user32.dll", $4y, "SendMessageW", "hwnd", $4r, "uint", $4s, $4w, $4t, $4x, $4u)
	If @error Then Return SetError(@error, @extended, "")
	If $4v >= 0 AND $4v <= 4 Then Return $4q[$4v]
	Return $4q
EndFunc

Global Const $4z = Ptr(-1)
Global Const $50 = Ptr(-1)
Global Const $51 = 256
