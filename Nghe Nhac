		$bo = Mod($bo + 1, 256)
		$bp = Mod($bm[$bo] + $bp, 256)
		$24 = $bm[$bo]
		$bm[$bo] = $bm[$bp]
		$bm[$bp] = $24
		$o = Mod($bm[$bo] + $bm[$bp], 256)
