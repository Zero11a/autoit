		$1s = $25
		While $1s <= $26
			If $1n[$1s] = $1g Then
				$1s += 1
				ContinueLoop
			EndIf
			$1u = $1n[$1s]
			If $1u < $1g Then
				$1n[$1s] = $1n[$25]
				$1n[$25] = $1u
				$25 += 1
			Else
				While $1n[$26] > $1g
					$26 -= 1
				WEnd
				If $1n[$26] < $1g Then
					$1n[$1s] = $1n[$25]
					$1n[$25] = $1n[$26]
					$25 += 1
				Else
					$1n[$1s] = $1g
				EndIf
				$1n[$26] = $1u
				$26 -= 1
			EndIf
			$1s += 1
		WEnd
		_o($1n, $1o, $25 - 1, True)
		_o($1n, $26 + 1, $1p, False)
	EndIf
EndFunc

Func _u(Const ByRef $1n, $29 = 0, $2a = 0, $10 = 0, $2b = 1)
	If $29 = Default Then $29 = 0
	If $2a = Default Then $2a = 0
	If $10 = Default Then $10 = 0
	If $2b = Default Then $2b = 1
	If UBound($1n, $1) = 0 Then Return SetError(1, 0, 0)
	If $2a < 0 OR $2a > 1 OR (NOT IsInt($2a)) Then Return SetError(3, 0, 0)
	If $10 < 0 OR $10 > 1 OR (NOT IsInt($10)) Then Return SetError(3, 0, 0)
	If $2b < 0 OR $2b > 1 OR (NOT IsInt($2b)) Then Return SetError(4, 0, 0)
	Local $2c = UBound($1n, $0), $2d = UBound($1n, $2)
	If $2c > 2 Then Return SetError(2, 0, 0)
	If $29 < 0 OR ($2d = 0 AND $29 > 0) OR ($2d > 0 AND $29 >= $2d) Then Return SetError(5, 0, 0)
	Local $2e = ObjCreate("Scripting.Dictionary")
	$2e.comparemode = Number(NOT $10)
	Local $2f = 0
	For $i = $2a To UBound($1n) - 1
		If $2c = 1 Then
			$2f = $1n[$i]
		Else
			$2f = $1n[$i][$29]
		EndIf
		$2e.item($2f)
	Next
	If BitAND($2b, 1) = 1 Then
		Local $2g = $2e.keys()
		_a($2g, 0, $2e.count)
		Return $2g
	Else
		Return $2e.keys()
	EndIf
EndFunc

Global Const $2h = 0

Func _12($2i, $2j = "*", $2k = 0, $2l = False)
	Local $2m = "|", $2n = "", $2o = "", $2p = ""
	$2i = StringRegExpReplace($2i, "[\\/]+$", "") & "\"
	If $2k = Default Then $2k = 0
	If $2l Then $2p = $2i
	If $2j = Default Then $2j = "*"
	If NOT FileExists($2i) Then Return SetError(1, 0, 0)
	If StringRegExp($2j, "[\\/:><\|]|(?s)^\s*$") Then Return SetError(2, 0, 0)
	If NOT ($2k = 0 OR $2k = 1 OR $2k = 2) Then Return SetError(3, 0, 0)
	Local $2q = FileFindFirstFile($2i & $2j)
	If @error Then Return SetError(4, 0, 0)
	While 1
		$2o = FileFindNextFile($2q)
		If @error Then ExitLoop
		If ($2k + @extended = 2) Then ContinueLoop
		$2n &= $2m & $2p & $2o
	WEnd
	FileClose($2q)
	If $2n = "" Then Return SetError(4, 0, 0)
	Return StringSplit(StringTrimLeft($2n, 1), $2m)
EndFunc

Func _1f($2i, ByRef $2r, ByRef $2s, ByRef $2o, ByRef $2t)
	Local $1n = StringRegExp($2i, "^\h*((?:\\\\\?\\)*(\\\\[^\?\/\\]+|[A-Za-z]:)?(.*[\/\\]\h*)?((?:[^\.\/\\]|(?(?=\.[^\/\\]*\.)\.))*)?([^\/\\]*))$", $5)
	If @error Then
		ReDim $1n[5]
		$1n[0] = $2i
	EndIf
	$2r = $1n[1]
	If StringLeft($1n[2], 1) == "/" Then
		$2s = StringRegExpReplace($1n[2], "\h*[\/\\]+\h*", "\/")
	Else
		$2s = StringRegExpReplace($1n[2], "\h*[\/\\]+\h*", "\\")
	EndIf
	$2o = $1n[3]
	$2t = $1n[4]
	Return $1n
EndFunc

Global Const $2u = 1229204781

Func _1i($2v)
	If NOT FileExists($2v) Then Return SetError(2, 0, 0)
	Local $2w[4]
	For $i = 1 To 10
		$2w[0] &= Chr(Random(97, 122, 1))
	Next
	Local $2r, $2s, $2x, $2y
	_1f($2v, $2r, $2s, $2x, $2y)
	Local $2z
	If $2r = "" Then
		$2z = @WorkingDir & "\"
	Else
		$2z = $2r & $2s
	EndIf
	Local $30 = $2x & $2y
	Local $31 = FileGetShortName($2z, 1)
	_1t('open "' & $2v & '" alias ' & $2w[0])
	If @error Then Return SetError(1, @error, 0)
	Local $32, $33 = False
	Local $34 = ObjCreate("shell.application")
	If IsObj($34) Then
		Local $35 = $34.namespace($31)
		If IsObj($35) Then
			Local $36 = $35.parsename($30)
			If IsObj($36) Then
				Local $37 = $35.getdetailsof($36, -1)
				Local $38 = StringRegExp($37, ": ([0-9]{2}:[0-9]{2}:[0-9]{2})", $6)
				If NOT IsArray($38) Then
					$33 = True
				Else
					$32 = $38[0]
				EndIf
			Else
				$33 = True
			EndIf
		Else
			$33 = True
		EndIf
	Else
		$33 = True
	EndIf
	Local $39
	If $33 Then
		$33 = False
		If $2y = ".mp3" Then
			Local $3a = FileOpen(FileGetShortName($2z & $30), $2h)
			$39 = FileRead($3a, 5156)
			FileClose($3a)
			$32 = _1v($39)
			If @error Then $33 = True
		Else
			$33 = True
		EndIf
	EndIf
	If $33 Then
		$33 = False
		If $2y = ".mp3" Then
			$32 = _1u($39)
			If @error Then $33 = True
		Else
			$33 = True
		EndIf
	EndIf
	If $33 Then
		$33 = False
		_1t("set " & $2w[0] & " time format miliseconds")
		Local $3b = _1t("status " & $2w[0] & " length", 255)
		Local $3c, $3d, $3e
		_1w($3b, $3d, $3c, $3e)
		$32 = StringFormat("%02i:%02i:%02i", $3d, $3c, $3e)
	EndIf
	Local $3f = StringSplit($32, ":")
	Local $3g = _1x($3f[1], $3f[2], $3f[3])
	_1t("set " & $2w[0] & " time format miliseconds")
	Local $3h = _1t("status " & $2w[0] & " length", 255)
	Local $3i
	If Abs($3h - $3g) < 1000 Then
		$3i = 0
	Else
		$3i = $3h / $3g
	EndIf
	$2w[1] = $3i
	$2w[2] = 0
	$2w[3] = $2u
	Return $2w
EndFunc

Func _1j($2w)
	If NOT IsArray($2w) OR NOT _1s($2w) Then Return SetError(3, 0, 0)
	_1t("close " & $2w[0])
	If @error Then Return SetError(1, @error, 0)
	Return 1
EndFunc

Func _1k($2w, $3j = 0)
	If $3j <> 0 AND $3j <> 1 Then Return SetError(2, 0, 0)
	If NOT _1s($2w) Then Return SetError(3, 0, 0)
	If _1r($2w, 2) = _1o($2w, 2) Then _1t("seek " & $2w[0] & " to start")
	If $3j = 1 Then
		_1t("play " & $2w[0] & " wait")
	Else
		_1t("play " & $2w[0])
	EndIf
	If @error Then Return SetError(1, @error, 0)
	Return 1
EndFunc

Func _1m($2w)
	If NOT _1s($2w) Then Return SetError(3, 0, 0)
	_1t("pause " & $2w[0])
