	Return $5k
EndFunc

Func _d5($5m, $5n)
	If NOT IsNumber($5m) Then Return SetError(1, 0, 0)
	If NOT IsNumber($5n) Then Return SetError(2, 0, 0)
	Return ($5m > $5n) ? $5m : $5n
EndFunc

