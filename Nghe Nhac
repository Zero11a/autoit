Func _b0($4r, $5h = 0)
	If $5h = 0 Then $5h = 0
	If IsHWnd($4r) Then
		If $5h = 0 Then $5h = _2j($4r, $5g, $5a, 0, 0, "wparam", "lparam", "handle")
