	Local $3s = Number(_1t("status " & $2w[0] & " position", 255))
	If $2w[1] <> 0 Then
		$3s -= $2w[2]
	EndIf
	If $3k = 2 Then Return $3s
	Local $3t, $3u, $3v
	_1w($3s, $3u, $3t, $3v)
	Local $3w = StringFormat("%02i:%02i:%02i", $3u, $3t, $3v)
	Return $3w
EndFunc

Func _1s(ByRef $2w)
	If NOT IsArray($2w) Then
		If NOT FileExists($2w) Then Return 0
		Local $3x = FileGetShortName($2w)
		Dim $2w[4] = [$3x, 0, 0, $2u]
	Else
		If UBound($2w) <> 4 AND $2w[3] <> $2u Then Return 0
	EndIf
	Return 1
EndFunc

Func _1t($3y, $3z = 0)
	Local $40 = DllCall("winmm.dll", "dword", "mciSendStringW", "wstr", $3y, "wstr", "", "uint", $3z, "ptr", 0)
	If @error Then Return SetError(@error, @extended, "")
	If $40[0] Then Return SetError(10, $40[0], $40[2])
	Return $40[2]
EndFunc

Func _1u($39)
	If StringLeft($39, 10) <> "0x49443303" Then Return SetError(1, 0, 0)
	Local $41 = StringInStr($39, "544C454E") + 21
	$39 = StringTrimLeft($39, $41)
	Local $42 = ""
	For $i = 1 To 32 Step 2
		If StringMid($39, $i, 2) = "00" Then
			ExitLoop
		Else
			$42 &= StringMid($39, $i, 2)
		EndIf
	Next
	Local $43 = Number(BinaryToString("0x" & $42))
	If $43 <= 0 Then Return SetError(1, 0, 0)
	Local $44, $45, $46
	_1w($43, $44, $45, $46)
	Return StringFormat("%02i:%02i:%02i", $44, $45, $46)
EndFunc

Func _1v($39)
	Local $47 = StringInStr($39, "58696E67")
	If $47 = 0 Then Return SetError(1, 0, 0)
	Local $48, $2b = Number("0x" & StringMid($39, $47 + 14, 2))
	If BitAND($2b, 1) = 1 Then
		$48 = Number("0x" & StringMid($39, $47 + 16, 8))
	Else
		Return SetError(1, 0, 0)
	EndIf
	Local $49 = StringMid($39, $47 - 72, 8)
	Local $4a = Number("0x" & StringMid($49, 4, 1))
	Local $4b = Number("0x" & StringMid($49, 6, 1))
	Local $4c = BitAND($4a, 8)
	Local $4d = BitAND($4a, 6)
	Local $4e
	Switch $4d
		Case 6
			$4e = 384
		Case 4
			$4e = 1152
		Case 2
			Switch $4c
				Case 8
					$4e = 1152
				Case 0
					$4e = 576
				Case Else
					$4e = 0
			EndSwitch
		Case Else
			$4e = 0
	EndSwitch
	If $4e = 0 Then Return SetError(1, 0, 0)
	Local $4f, $4g = BitAND($4b, 12)
	Switch $4g
		Case 0
			$4f = 44100
		Case 4
			$4f = 48000
		Case 8
			$4f = 32000
		Case Else
			$4f = 0
	EndSwitch
	If $4f = 0 Then Return SetError(1, 0, 0)
	If $4c = 0 Then $4f = $4f / 2
	Local $43 = Int(($48 * $4e / $4f) * 1000)
	Local $4h, $4i, $46
	_1w($43, $4h, $4i, $46)
	Return StringFormat("%02i:%02i:%02i", $4h, $4i, $46)
EndFunc

Func _1w($4j, ByRef $4k, ByRef $4l, ByRef $4m)
	If Number($4j) < 0 Then Return SetError(1, 0, 0)
	If Number($4j) = 0 Then
		$4k = 0
		$4j = 0
		$4l = 0
		$4m = 0
		Return 1
	EndIf
	$4j = Round($4j / 1000)
	$4k = Int($4j / 3600)
	$4j = Mod($4j, 3600)
	$4l = Int($4j / 60)
	$4m = Round(Mod($4j, 60))
	Return 1
EndFunc

Func _1x($4k = @HOUR, $4l = @MIN, $4m = @SEC)
	If NOT (StringIsInt($4k) AND StringIsInt($4l) AND StringIsInt($4m)) Then Return SetError(1, 0, 0)
	Return 1000 * ((3600 * $4k) + (60 * $4l) + $4m)
EndFunc

