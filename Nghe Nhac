Opt("MustDeclareVars", 1)
Opt("GUIResizeMode", 802)
Opt("MouseCoordMode", 2)
Opt("GUIOnEventMode", 1)
Global Const $0 = 0
Global Const $1 = 1
Global Const $2 = 2
Global Const $3 = 1
Global Const $4 = 2
Global Const $5 = 1
Global Const $6 = 3
Global Const $7 = 0
Global Const $8 = 1

Func _0(ByRef $9, $a, $b = 0, $c = "|", $d = @CRLF, $e = 0)
	If $b = Default Then $b = 0
	If $c = Default Then $c = "|"
	If $d = Default Then $d = @CRLF
	If $e = Default Then $e = 0
	If NOT IsArray($9) Then Return SetError(1, 0, -1)
	Local $f = UBound($9, $1)
	Switch UBound($9, $0)
		Case 1
			If IsArray($a) Then
				If UBound($a, $0) <> 1 Then Return SetError(5, 0, -1)
				$e = 0
			Else
				Local $g = StringSplit($a, $c, $4 + $3)
				If UBound($g, $1) = 1 Then
					$g[0] = $a
					$e = 0
				EndIf
				$a = $g
			EndIf
			Local $h = UBound($a, $1)
			ReDim $9[$f + $h]
			For $i = 0 To $h - 1
				If IsFunc($e) Then
					$9[$f + $i] = $e($a[$i])
				Else
					$9[$f + $i] = $a[$i]
				EndIf
			Next
			Return $f + $h - 1
		Case 2
			Local $j = UBound($9, $2)
			If $b < 0 OR $b > $j - 1 Then Return SetError(4, 0, -1)
			Local $k, $l
			If IsArray($a) Then
				If UBound($a, $0) <> 2 Then Return SetError(5, 0, -1)
				$k = UBound($a, $1)
				$l = UBound($a, $2)
				$e = 0
			Else
				Local $m = StringSplit($a, $d, $4 + $3)
				$k = UBound($m, $1)
				StringReplace($m[0], $c, "")
				$l = @extended + 1
				Local $g[$k][$l], $n
				For $i = 0 To $k - 1
					$n = StringSplit($m[$i], $c, $4 + $3)
					For $o = 0 To $l - 1
						$g[$i][$o] = $n[$o]
					Next
				Next
				$a = $g
			EndIf
			If UBound($a, $2) + $b > UBound($9, $2) Then Return SetError(3, 0, -1)
			ReDim $9[$f + $k][$j]
			For $p = 0 To $k - 1
				For $o = 0 To $j - 1
					If $o < $b Then
						$9[$p + $f][$o] = ""
					ElseIf $o - $b > $l - 1 Then
						$9[$p + $f][$o] = ""
					Else
						If IsFunc($e) Then
							$9[$p + $f][$o] = $e($a[$p][$o - $b])
						Else
							$9[$p + $f][$o] = $a[$p][$o - $b]
						EndIf
					EndIf
				Next
			Next
		Case Else
			Return SetError(2, 0, -1)
	EndSwitch
	Return UBound($9, $1) - 1
EndFunc

Func _6(ByRef $9, $q)
	If NOT IsArray($9) Then Return SetError(1, 0, -1)
	Local $f = UBound($9, $1) - 1
	If IsArray($q) Then
		If UBound($q, $0) <> 1 OR UBound($q, $1) < 2 Then Return SetError(4, 0, -1)
	Else
		Local $r, $m, $n
		$q = StringStripWS($q, 8)
		$m = StringSplit($q, ";")
		$q = ""
		For $i = 1 To $m[0]
			If NOT StringRegExp($m[$i], "^\d+(-\d+)?$") Then Return SetError(3, 0, -1)
			$n = StringSplit($m[$i], "-")
			Switch $n[0]
				Case 1
					$q &= $n[1] & ";"
				Case 2
					If Number($n[2]) >= Number($n[1]) Then
						$r = $n[1] - 1
						Do
							$r += 1
							$q &= $r & ";"
						Until $r = $n[2]
					EndIf
			EndSwitch
		Next
		$q = StringSplit(StringTrimRight($q, 1), ";")
	EndIf
	If $q[1] < 0 OR $q[$q[0]] > $f Then Return SetError(5, 0, -1)
	Local $s = 0
	Switch UBound($9, $0)
		Case 1
			For $i = 1 To $q[0]
				$9[$q[$i]] = ChrW(64177)
			Next
			For $t = 0 To $f
				If $9[$t] == ChrW(64177) Then
					ContinueLoop
				Else
					If $t <> $s Then
						$9[$s] = $9[$t]
					EndIf
					$s += 1
				EndIf
			Next
			ReDim $9[$f - $q[0] + 1]
		Case 2
			Local $j = UBound($9, $2) - 1
			For $i = 1 To $q[0]
				$9[$q[$i]][0] = ChrW(64177)
			Next
			For $t = 0 To $f
				If $9[$t][0] == ChrW(64177) Then
					ContinueLoop
				Else
					If $t <> $s Then
						For $o = 0 To $j
							$9[$s][$o] = $9[$t][$o]
						Next
					EndIf
					$s += 1
				EndIf
			Next
			ReDim $9[$f - $q[0] + 1][$j + 1]
		Case Else
			Return SetError(2, 0, False)
	EndSwitch
	Return UBound($9, $1)
EndFunc

Func _a(ByRef $9, $q, $a = "", $b = 0, $c = "|", $d = @CRLF, $e = 0)
	If $a = Default Then $a = ""
	If $b = Default Then $b = 0
	If $c = Default Then $c = "|"
	If $d = Default Then $d = @CRLF
	If $e = Default Then $e = 0
	If NOT IsArray($9) Then Return SetError(1, 0, -1)
	Local $f = UBound($9, $1) - 1
	Local $m, $n
	If IsArray($q) Then
		If UBound($q, $0) <> 1 OR UBound($q, $1) < 2 Then Return SetError(4, 0, -1)
	Else
		Local $r
		$q = StringStripWS($q, 8)
		$m = StringSplit($q, ";")
		$q = ""
		For $i = 1 To $m[0]
			If NOT StringRegExp($m[$i], "^\d+(-\d+)?$") Then Return SetError(3, 0, -1)
			$n = StringSplit($m[$i], "-")
			Switch $n[0]
				Case 1
					$q &= $n[1] & ";"
				Case 2
					If Number($n[2]) >= Number($n[1]) Then
						$r = $n[1] - 1
						Do
							$r += 1
							$q &= $r & ";"
						Until $r = $n[2]
					EndIf
			EndSwitch
		Next
		$q = StringSplit(StringTrimRight($q, 1), ";")
	EndIf
	If $q[1] < 0 OR $q[$q[0]] > $f Then Return SetError(5, 0, -1)
	For $i = 2 To $q[0]
		If $q[$i] < $q[$i - 1] Then Return SetError(3, 0, -1)
	Next
	Local $s = $f + $q[0]
	Local $u = $q[0]
	Local $v = $q[$u]
	Switch UBound($9, $0)
		Case 1
			ReDim $9[$f + $q[0] + 1]
			If IsArray($a) Then
				If UBound($a, $0) <> 1 Then Return SetError(5, 0, -1)
				$e = 0
			Else
				Local $g = StringSplit($a, $c, $4 + $3)
				If UBound($g, $1) = 1 Then
					$g[0] = $a
					$e = 0
				EndIf
				$a = $g
			EndIf
			For $w = $f To 0 Step -1
				$9[$s] = $9[$w]
				$s -= 1
				$v = $q[$u]
				While $w = $v
					If $u <= UBound($a, $1) Then
						If IsFunc($e) Then
							$9[$s] = $e($a[$u - 1])
						Else
							$9[$s] = $a[$u - 1]
						EndIf
					Else
						$9[$s] = ""
					EndIf
					$s -= 1
					$u -= 1
					If $u = 0 Then ExitLoop 2
					$v = $q[$u]
				WEnd
			Next
		Case 2
			Local $j = UBound($9, $2)
			If $b < 0 OR $b > $j - 1 Then Return SetError(6, 0, -1)
			Local $k, $l
			If IsArray($a) Then
				If UBound($a, $0) <> 2 Then Return SetError(7, 0, -1)
				$k = UBound($a, $1)
				$l = UBound($a, $2)
				$e = 0
			Else
				$m = StringSplit($a, $d, $4 + $3)
				$k = UBound($m, $1)
				StringReplace($m[0], $c, "")
				$l = @extended + 1
				Local $g[$k][$l]
				For $i = 0 To $k - 1
					$n = StringSplit($m[$i], $c, $4 + $3)
					For $o = 0 To $l - 1
						$g[$i][$o] = $n[$o]
					Next
				Next
				$a = $g
			EndIf
			If UBound($a, $2) + $b > UBound($9, $2) Then Return SetError(8, 0, -1)
			ReDim $9[$f + $q[0] + 1][$j]
			For $w = $f To 0 Step -1
				For $o = 0 To $j - 1
					$9[$s][$o] = $9[$w][$o]
				Next
				$s -= 1
				$v = $q[$u]
				While $w = $v
					For $o = 0 To $j - 1
						If $o < $b Then
							$9[$s][$o] = ""
						ElseIf $o - $b > $l - 1 Then
							$9[$s][$o] = ""
						Else
							If $u - 1 < $k Then
								If IsFunc($e) Then
									$9[$s][$o] = $e($a[$u - 1][$o - $b])
								Else
									$9[$s][$o] = $a[$u - 1][$o - $b]
								EndIf
							Else
								$9[$s][$o] = ""
							EndIf
						EndIf
					Next
					$s -= 1
					$u -= 1
					If $u = 0 Then ExitLoop 2
					$v = $q[$u]
				WEnd
			Next
		Case Else
			Return SetError(2, 0, -1)
	EndSwitch
	Return UBound($9, $1)
EndFunc

Func _i(ByRef $9, $b = 0, $x = 0)
	If $b = Default Then $b = 0
	If $x = Default Then $x = 0
	If NOT IsArray($9) Then Return SetError(1, 0, 0)
	If UBound($9, $0) <> 1 Then Return SetError(3, 0, 0)
	If NOT UBound($9) Then Return SetError(4, 0, 0)
	Local $y, $0z = UBound($9) - 1
	If $x < 1 OR $x > $0z Then $x = $0z
	If $b < 0 Then $b = 0
	If $b > $x Then Return SetError(2, 0, 0)
	For $i = $b To Int(($b + $x - 1) / 2)
		$y = $9[$i]
		$9[$i] = $9[$x]
		$9[$x] = $y
		$x -= 1
	Next
	Return 1
EndFunc

Func _j(Const ByRef $9, $a, $b = 0, $x = 0, $10 = 0, $11 = 0, $12 = 1, $13 = -1, $14 = False)
	If $b = Default Then $b = 0
	If $x = Default Then $x = 0
	If $10 = Default Then $10 = 0
	If $11 = Default Then $11 = 0
	If $12 = Default Then $12 = 1
	If $13 = Default Then $13 = -1
	If $14 = Default Then $14 = False
	If NOT IsArray($9) Then Return SetError(1, 0, -1)
	Local $f = UBound($9) - 1
	If $f = -1 Then Return SetError(3, 0, -1)
	Local $j = UBound($9, $2) - 1
	Local $15 = False
	If $11 = 2 Then
		$11 = 0
		$15 = True
	EndIf
	If $14 Then
		If UBound($9, $0) = 1 Then Return SetError(5, 0, -1)
		If $x < 1 OR $x > $j Then $x = $j
		If $b < 0 Then $b = 0
		If $b > $x Then Return SetError(4, 0, -1)
	Else
		If $x < 1 OR $x > $f Then $x = $f
		If $b < 0 Then $b = 0
		If $b > $x Then Return SetError(4, 0, -1)
	EndIf
	Local $16 = 1
	If NOT $12 Then
		Local $17 = $b
		$b = $x
		$x = $17
		$16 = -1
	EndIf
	Switch UBound($9, $0)
		Case 1
			If NOT $11 Then
				If NOT $10 Then
					For $i = $b To $x Step $16
						If $15 AND VarGetType($9[$i]) <> VarGetType($a) Then ContinueLoop
						If $9[$i] = $a Then Return $i
					Next
				Else
					For $i = $b To $x Step $16
						If $15 AND VarGetType($9[$i]) <> VarGetType($a) Then ContinueLoop
						If $9[$i] == $a Then Return $i
					Next
				EndIf
			Else
				For $i = $b To $x Step $16
					If $11 = 3 Then
						If StringRegExp($9[$i], $a) Then Return $i
					Else
						If StringInStr($9[$i], $a, $10) > 0 Then Return $i
					EndIf
				Next
			EndIf
		Case 2
			Local $18
			If $14 Then
				$18 = $f
				If $13 > $18 Then $13 = $18
				If $13 < 0 Then
					$13 = 0
				Else
					$18 = $13
				EndIf
			Else
				$18 = $j
				If $13 > $18 Then $13 = $18
				If $13 < 0 Then
					$13 = 0
				Else
					$18 = $13
				EndIf
			EndIf
			For $o = $13 To $18
				If NOT $11 Then
					If NOT $10 Then
						For $i = $b To $x Step $16
							If $14 Then
								If $15 AND VarGetType($9[$o][$o]) <> VarGetType($a) Then ContinueLoop
								If $9[$o][$i] = $a Then Return $i
							Else
								If $15 AND VarGetType($9[$i][$o]) <> VarGetType($a) Then ContinueLoop
								If $9[$i][$o] = $a Then Return $i
							EndIf
						Next
					Else
						For $i = $b To $x Step $16
							If $14 Then
								If $15 AND VarGetType($9[$o][$i]) <> VarGetType($a) Then ContinueLoop
								If $9[$o][$i] == $a Then Return $i
							Else
								If $15 AND VarGetType($9[$i][$o]) <> VarGetType($a) Then ContinueLoop
								If $9[$i][$o] == $a Then Return $i
							EndIf
						Next
					EndIf
				Else
					For $i = $b To $x Step $16
						If $11 = 3 Then
							If $14 Then
								If StringRegExp($9[$o][$i], $a) Then Return $i
							Else
								If StringRegExp($9[$i][$o], $a) Then Return $i
							EndIf
						Else
							If $14 Then
								If StringInStr($9[$o][$i], $a, $10) > 0 Then Return $i
							Else
								If StringInStr($9[$i][$o], $a, $10) > 0 Then Return $i
							EndIf
						EndIf
					Next
				EndIf
			Next
		Case Else
			Return SetError(2, 0, -1)
	EndSwitch
	Return SetError(6, 0, -1)
EndFunc

Func _k(ByRef $9, $19 = 0, $1a = 0, $1b = -1)
	If $19 = Default Then $19 = 0
	If $1a = Default Then $1a = 0
	If $1b = Default Then $1b = -1
	If NOT IsArray($9) Then Return SetError(1, 0, -1)
	Local $f = UBound($9, $1)
	If $1a = 0 Then $1a = $f - 1
	If $19 < 0 OR $19 > $f - 1 Then Return SetError(3, 0, -1)
	If $1a < 1 OR $1a > $f - 1 Then Return SetError(3, 0, -1)
	If $19 > $1a Then Return SetError(4, 0, -1)
	Local $y, $1c
	Switch UBound($9, $0)
		Case 1
			For $i = $1a To $19 + 1 Step -1
				$1c = Random($19, $i, 1)
				$y = $9[$i]
				$9[$i] = $9[$1c]
				$9[$1c] = $y
			Next
			Return 1
		Case 2
			Local $j = UBound($9, $2)
			If $1b < -1 OR $1b > $j - 1 Then Return SetError(5, 0, -1)
			Local $1d, $1e
			If $1b = -1 Then
				$1d = 0
				$1e = $j - 1
			Else
				$1d = $1b
				$1e = $1b
			EndIf
			For $i = $1a To $19 + 1 Step -1
				$1c = Random($19, $i, 1)
				For $o = $1d To $1e
					$y = $9[$i][$o]
					$9[$i][$o] = $9[$1c][$o]
					$9[$1c][$o] = $y
				Next
			Next
			Return 1
		Case Else
			Return SetError(2, 0, -1)
	EndSwitch
EndFunc

Func _l(ByRef $9, $1f = 0, $b = 0, $x = 0, $13 = 0, $1g = 0)
	If $1f = Default Then $1f = 0
	If $b = Default Then $b = 0
	If $x = Default Then $x = 0
	If $13 = Default Then $13 = 0
	If $1g = Default Then $1g = 0
	If NOT IsArray($9) Then Return SetError(1, 0, 0)
	Local $0z = UBound($9) - 1
	If $0z = -1 Then Return SetError(5, 0, 0)
	If $x = Default Then $x = 0
	If $x < 1 OR $x > $0z OR $x = Default Then $x = $0z
	If $b < 0 OR $b = Default Then $b = 0
	If $b > $x Then Return SetError(2, 0, 0)
	If $1f = Default Then $1f = 0
	If $1g = Default Then $1g = 0
	If $13 = Default Then $13 = 0
	Switch UBound($9, $0)
		Case 1
			If $1g Then
				_o($9, $b, $x)
			Else
				_m($9, $b, $x)
			EndIf
			If $1f Then _i($9, $b, $x)
		Case 2
			If $1g Then Return SetError(6, 0, 0)
			Local $1h = UBound($9, $2) - 1
			If $13 > $1h Then Return SetError(3, 0, 0)
			If $1f Then
				$1f = -1
			Else
				$1f = 1
			EndIf
			_n($9, $1f, $b, $x, $13, $1h)
		Case Else
			Return SetError(4, 0, 0)
	EndSwitch
	Return 1
EndFunc

Func _m(ByRef $9, Const ByRef $b, Const ByRef $x)
	If $x <= $b Then Return 
	Local $y
	If ($x - $b) < 15 Then
		Local $1i
		For $i = $b + 1 To $x
			$y = $9[$i]
			If IsNumber($y) Then
				For $o = $i - 1 To $b Step -1
					$1i = $9[$o]
					If ($y >= $1i AND IsNumber($1i)) OR (NOT IsNumber($1i) AND StringCompare($y, $1i) >= 0) Then ExitLoop
					$9[$o + 1] = $1i
				Next
			Else
				For $o = $i - 1 To $b Step -1
					If (StringCompare($y, $9[$o]) >= 0) Then ExitLoop
					$9[$o + 1] = $9[$o]
				Next
			EndIf
			$9[$o + 1] = $y
		Next
		Return 
	EndIf
	Local $1j = $b, $1k = $x, $1l = $9[Int(($b + $x) / 2)], $1m = IsNumber($1l)
	Do
		If $1m Then
			While ($9[$1j] < $1l AND IsNumber($9[$1j])) OR (NOT IsNumber($9[$1j]) AND StringCompare($9[$1j], $1l) < 0)
				$1j += 1
			WEnd
			While ($9[$1k] > $1l AND IsNumber($9[$1k])) OR (NOT IsNumber($9[$1k]) AND StringCompare($9[$1k], $1l) > 0)
				$1k -= 1
			WEnd
		Else
			While (StringCompare($9[$1j], $1l) < 0)
				$1j += 1
			WEnd
			While (StringCompare($9[$1k], $1l) > 0)
				$1k -= 1
			WEnd
		EndIf
		If $1j <= $1k Then
			$y = $9[$1j]
			$9[$1j] = $9[$1k]
			$9[$1k] = $y
			$1j += 1
			$1k -= 1
		EndIf
	Until $1j > $1k
	_m($9, $b, $1k)
	_m($9, $1j, $x)
EndFunc

Func _n(ByRef $9, Const ByRef $16, Const ByRef $b, Const ByRef $x, Const ByRef $13, Const ByRef $1h)
	If $x <= $b Then Return 
	Local $y, $1j = $b, $1k = $x, $1l = $9[Int(($b + $x) / 2)][$13], $1m = IsNumber($1l)
	Do
		If $1m Then
			While ($16 * ($9[$1j][$13] - $1l) < 0 AND IsNumber($9[$1j][$13])) OR (NOT IsNumber($9[$1j][$13]) AND $16 * StringCompare($9[$1j][$13], $1l) < 0)
				$1j += 1
			WEnd
			While ($16 * ($9[$1k][$13] - $1l) > 0 AND IsNumber($9[$1k][$13])) OR (NOT IsNumber($9[$1k][$13]) AND $16 * StringCompare($9[$1k][$13], $1l) > 0)
				$1k -= 1
			WEnd
		Else
			While ($16 * StringCompare($9[$1j][$13], $1l) < 0)
				$1j += 1
			WEnd
			While ($16 * StringCompare($9[$1k][$13], $1l) > 0)
				$1k -= 1
			WEnd
		EndIf
		If $1j <= $1k Then
			For $i = 0 To $1h
				$y = $9[$1j][$i]
				$9[$1j][$i] = $9[$1k][$i]
				$9[$1k][$i] = $y
			Next
			$1j += 1
			$1k -= 1
		EndIf
	Until $1j > $1k
	_n($9, $16, $b, $1k, $13, $1h)
	_n($9, $16, $1j, $x, $13, $1h)
EndFunc

Func _o(ByRef $1n, $1o, $1p, $1q = True)
	If $1o > $1p Then Return 
	Local $1r = $1p - $1o + 1
	Local $i, $o, $1s, $1t, $1u, $1v, $1w, $1x
	If $1r < 45 Then
		If $1q Then
			$i = $1o
			While $i < $1p
				$o = $i
				$1t = $1n[$i + 1]
				While $1t < $1n[$o]
					$1n[$o + 1] = $1n[$o]
					$o -= 1
					If $o + 1 = $1o Then ExitLoop
				WEnd
				$1n[$o + 1] = $1t
				$i += 1
			WEnd
		Else
			While 1
				If $1o >= $1p Then Return 1
				$1o += 1
				If $1n[$1o] < $1n[$1o - 1] Then ExitLoop
			WEnd
			While 1
				$1s = $1o
				$1o += 1
				If $1o > $1p Then ExitLoop
				$1v = $1n[$1s]
				$1w = $1n[$1o]
				If $1v < $1w Then
					$1w = $1v
					$1v = $1n[$1o]
				EndIf
				$1s -= 1
				While $1v < $1n[$1s]
					$1n[$1s + 2] = $1n[$1s]
					$1s -= 1
				WEnd
				$1n[$1s + 2] = $1v
				While $1w < $1n[$1s]
					$1n[$1s + 1] = $1n[$1s]
					$1s -= 1
				WEnd
				$1n[$1s + 1] = $1w
				$1o += 1
			WEnd
			$1x = $1n[$1p]
			$1p -= 1
			While $1x < $1n[$1p]
				$1n[$1p + 1] = $1n[$1p]
				$1p -= 1
			WEnd
			$1n[$1p + 1] = $1x
		EndIf
		Return 1
	EndIf
	Local $1y = BitShift($1r, 3) + BitShift($1r, 6) + 1
	Local $1z, $20, $21, $22, $23, $24
	$21 = Ceiling(($1o + $1p) / 2)
	$20 = $21 - $1y
	$1z = $20 - $1y
	$22 = $21 + $1y
	$23 = $22 + $1y
	If $1n[$20] < $1n[$1z] Then
		$24 = $1n[$20]
		$1n[$20] = $1n[$1z]
		$1n[$1z] = $24
	EndIf
	If $1n[$21] < $1n[$20] Then
		$24 = $1n[$21]
		$1n[$21] = $1n[$20]
		$1n[$20] = $24
		If $24 < $1n[$1z] Then
			$1n[$20] = $1n[$1z]
			$1n[$1z] = $24
		EndIf
	EndIf
	If $1n[$22] < $1n[$21] Then
		$24 = $1n[$22]
		$1n[$22] = $1n[$21]
		$1n[$21] = $24
		If $24 < $1n[$20] Then
			$1n[$21] = $1n[$20]
			$1n[$20] = $24
			If $24 < $1n[$1z] Then
				$1n[$20] = $1n[$1z]
				$1n[$1z] = $24
			EndIf
		EndIf
	EndIf
	If $1n[$23] < $1n[$22] Then
		$24 = $1n[$23]
		$1n[$23] = $1n[$22]
		$1n[$22] = $24
		If $24 < $1n[$21] Then
			$1n[$22] = $1n[$21]
			$1n[$21] = $24
			If $24 < $1n[$20] Then
				$1n[$21] = $1n[$20]
				$1n[$20] = $24
				If $24 < $1n[$1z] Then
					$1n[$20] = $1n[$1z]
					$1n[$1z] = $24
				EndIf
			EndIf
		EndIf
	EndIf
	Local $25 = $1o
	Local $26 = $1p
	If (($1n[$1z] <> $1n[$20]) AND ($1n[$20] <> $1n[$21]) AND ($1n[$21] <> $1n[$22]) AND ($1n[$22] <> $1n[$23])) Then
		Local $27 = $1n[$20]
		Local $28 = $1n[$22]
		$1n[$20] = $1n[$1o]
		$1n[$22] = $1n[$1p]
		Do
			$25 += 1
		Until $1n[$25] >= $27
		Do
			$26 -= 1
		Until $1n[$26] <= $28
		$1s = $25
		While $1s <= $26
			$1u = $1n[$1s]
			If $1u < $27 Then
				$1n[$1s] = $1n[$25]
				$1n[$25] = $1u
				$25 += 1
			ElseIf $1u > $28 Then
				While $1n[$26] > $28
					$26 -= 1
					If $26 + 1 = $1s Then ExitLoop 2
				WEnd
				If $1n[$26] < $27 Then
					$1n[$1s] = $1n[$25]
					$1n[$25] = $1n[$26]
					$25 += 1
				Else
					$1n[$1s] = $1n[$26]
				EndIf
				$1n[$26] = $1u
				$26 -= 1
			EndIf
			$1s += 1
		WEnd
		$1n[$1o] = $1n[$25 - 1]
		$1n[$25 - 1] = $27
		$1n[$1p] = $1n[$26 + 1]
		$1n[$26 + 1] = $28
		_o($1n, $1o, $25 - 2, True)
		_o($1n, $26 + 2, $1p, False)
		If ($25 < $1z) AND ($23 < $26) Then
			While $1n[$25] = $27
				$25 += 1
			WEnd
			While $1n[$26] = $28
				$26 -= 1
			WEnd
			$1s = $25
			While $1s <= $26
				$1u = $1n[$1s]
				If $1u = $27 Then
					$1n[$1s] = $1n[$25]
					$1n[$25] = $1u
					$25 += 1
				ElseIf $1u = $28 Then
					While $1n[$26] = $28
						$26 -= 1
						If $26 + 1 = $1s Then ExitLoop 2
					WEnd
					If $1n[$26] = $27 Then
						$1n[$1s] = $1n[$25]
						$1n[$25] = $27
						$25 += 1
					Else
						$1n[$1s] = $1n[$26]
					EndIf
					$1n[$26] = $1u
					$26 -= 1
				EndIf
				$1s += 1
			WEnd
		EndIf
		_o($1n, $25, $26, False)
	Else
