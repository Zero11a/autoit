EndFunc

Func _1n($2w)
	If NOT _1s($2w) Then Return SetError(3, 0, 0)
	_1t("resume " & $2w[0])
	If @error Then Return SetError(1, @error, 0)
	Return 1
EndFunc

Func _1o($2w, $3k = 1)
	If $3k <> 1 AND $3k <> 2 Then Return SetError(1, 0, 0)
	Local $3l = False
	If NOT IsArray($2w) Then
		If NOT FileExists($2w) Then Return SetError(3, 0, 0)
		$3l = True
		$2w = _1i($2w)
	Else
		If NOT _1s($2w) Then Return SetError(3, 0, 0)
	EndIf
	_1t("set " & $2w[0] & " time format miliseconds")
	Local $3b = Number(_1t("status " & $2w[0] & " length", 255))
	If $2w[1] <> 0 Then $3b = Round($3b / $2w[1])
	If $3l Then _1j($2w)
	If $3k = 2 Then Return $3b
	Local $3c, $3d, $3e
	_1w($3b, $3d, $3c, $3e)
	Local $3m = StringFormat("%02i:%02i:%02i", $3d, $3c, $3e)
	Return $3m
EndFunc

Func _1p(ByRef $2w, $3n, $3o, $3p)
	If NOT IsArray($2w) OR NOT _1s($2w) Then Return SetError(3, 0, 0)
	_1t("set " & $2w[0] & " time format miliseconds")
	Local $3q = $3p * 1000
	$3q += $3o * 60 * 1000
	$3q += $3n * 60 * 60 * 1000
	If $2w[1] <> 0 Then
		$2w[2] = Round($3q * $2w[1]) - $3q
		$3q = Round($3q * $2w[1])
	EndIf
	_1t("seek " & $2w[0] & " to " & $3q)
	Local $3r = @error
	If _1r($2w, 2) < 0 Then $2w[2] = 0
	If $3r Then Return SetError(1, $3r, 0)
	Return 1
EndFunc

Func _1q($2w)
	If NOT _1s($2w) Then Return SetError(3, 0, 0)
	Return _1t("status " & $2w[0] & " mode", 255)
