EndFunc

Func _vx($ib)
	If NOT IsArray($ib) Then Return SetError(1)
	For $i = 0 To 3
		GUICtrlSetData($h1[$i], $ib[$i])
		GUICtrlSetData($h1[$i + 4], $ib[$i])
	Next
EndFunc

Func _vy()
	_1q($gu)
	If @error Then Return SetError(1)
	If NOT IsArray($gx) Then Return SetError(2)
	If NOT IsArray($gm) Then Return SetError(3)
	If $gw > $gx[0][0] Then Return SetError(4)
	Local $c5, $ax, $c2
	$ax = StringTrimRight($gx[$gw][0], 4) & ".lrc"
	$c2 = _jg($ax, $gm, $gx[$gw][2], $gx[$gw][3])
	$gy = _jh($c2)
	If @error Then
		Dim $c5[4] = ["", "Không tìm thấy lời bài hát", "", ""]
		_vx($c5)
	EndIf
	$gz = 1
EndFunc

Func _vz()
	Local $i5, $cc, $hu, $ja, $jb, $jc
	$i5 = _1r($gu, 2)
	$cc = _w4($i5)
	If $cc <> GUICtrlRead($hi) Then
		GUICtrlSetData($hi, $cc)
		GUICtrlSetData($hl, _w4($gv - $i5))
		$ja = 295
		$jb = $i5 / $gv
		$hu = Int($jb * $ja)
		ControlMove($h2, "", $hj, 160, 270, $hu, 25)
		ControlMove($h2, "", $hk, 160 + $hu, 270, $ja - $hu, 25)
		_j0($h2, $jb * 100, 100)
	EndIf
	$jc = _ji($gy, $i5, $gz)
	If NOT @error Then
		$gz = @extended
		_vx($jc)
	EndIf
EndFunc

Func _w0()
	_1j($gu)
	If NOT IsArray($gx) Then Return SetError(1)
	Local $ic, $jd, $at
	$ic = GUICtrlRead($ho)
	If @error Then Return SetError(2)
	$gw = _by($ho, $ic) + 1
	$at = _w3($gw)
	If NOT FileExists($at) Then
		_vu(0)
		Return SetError(3)
	EndIf
	$gu = _1i($at)
	_1k($gu)
	$gv = _1o($gu, 2)
	WinSetTitle($h2, "", $gx[$gw][0] & " - Nghe nhạc")
	GUICtrlSetData($h7, $gx[$gw][2])
	GUICtrlSetData($h8, $gx[$gw][3])
	GUICtrlSetData($h9, _w4($gv))
	_vu(2)
	_vy()
EndFunc

Func _w1($i8)
	Local $je, $jf, $jg
	$i8 = _d5(_d6($i8, $gv - 1000), 0)
	_fr($i8, $je, $jf, $jg)
	_1p($gu, $je, $jf, $jg)
	_vz()
EndFunc

Func _w2($jh, $ji)
	If NOT IsArray($jh) OR NOT IsArray($ji) Then Return SetError(1)
	If UBound($jh, $0) <> 1 OR UBound($ji, $0) <> 1 Then Return SetError(2)
	If UBound($jh) <> UBound($ji) Then Return True
	For $i = 0 To UBound($jh) - 1
		_j($jh, $ji[$i])
		If @error Then Return True
	Next
	Return False
EndFunc

Func _w3($au)
	If NOT IsArray($gx) Then Return SetError(1)
	If NOT IsArray($gl) Then Return SetError(2)
	Local $jj
	If $au > $gx[0][0] Then Return SetError(3)
	$jj = $gx[$au][1]
	If $jj > UBound($gl) - 1 Then Return SetError(4)
	Return $gl[$jj] & "\" & $gx[$au][0]
EndFunc

Func _w4($jk)
	Local $je, $jf, $jg, $cc
	_fr($jk, $je, $jf, $jg)
	$cc = StringFormat("%02i:%02i:%02i", $je, $jf, $jg)
	Return $cc
EndFunc

Func _w5()
	GUIDelete($h2)
	GUIDelete($hp)
	If $gs Then
		For $i = $go To 0 Step -2
			SoundSetWaveVolume($i)
			Sleep(29)
		Next
	EndIf
	If $gu Then _1j($gu)
	Exit
EndFunc

Func _w6($jl, $jm, $jn = "Setting")
	Return IniRead($gk, $jn, $jl, $jm)
EndFunc

Func _w7($jl, $jo, $jn = "Setting")
	If NOT FileExists($gk) Then FileClose(FileOpen($gk, 2 + 8 + 128))
	Return IniWrite($gk, $jn, $jl, $jo)
EndFunc

Func _w8()
	GUISwitch($h2)
	If $ho Then GUICtrlDelete($ho)
	$ho = GUICtrlCreateTreeView(540, 50, 255, 245, 0)
	If NOT IsArray($gx) Then
		$h0 = 0
		GUICtrlSetData($hn, "Không tìm thấy bài hát nào")
		Return SetError(1)
	EndIf
	Dim $h0[$gx[0][0] + 1] = [$gx[0][0]]
	For $i = 1 To $h0[0]
		$h0[$i] = GUICtrlCreateTreeViewItem($gx[$i][0], $ho)
		GUICtrlSetOnEvent(-1, "_w0")
	Next
	GUICtrlSetData($hn, "Tổng cộng: " & $gx[0][0] & " bài hát")
EndFunc
