Func _fr($4j, ByRef $4k, ByRef $4l, ByRef $4m)
	If Number($4j) > 0 Then
		$4j = Int($4j / 1000)
		$4k = Int($4j / 3600)
		$4j = Mod($4j, 3600)
		$4l = Int($4j / 60)
		$4m = Mod($4j, 60)
		Return 1
	ElseIf Number($4j) = 0 Then
		$4k = 0
		$4j = 0
		$4l = 0
		$4m = 0
		Return 1
	Else
		Return SetError(1, 0, 0)
	EndIf
EndFunc

